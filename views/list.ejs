



<div class="container-fluid">
    <div class="row-fluid">
        <div class="span2">




        </div>
        <div class="span10">
            <h3>Found <%=total %>, showing <%=count_from %> to <%= count %></h3>
            <table class="table table-striped">
                <tr>  <h1><td>Address</td> <td>Detail</td></h1>
                <tr ng-repeat="entry in entries | filter:query">
                    <td> {{entry.address}}</td>
                    <td> {{entry.detail}}</td>


                </tr>

            </table>

            <script>
                function increment(){
                    document.getElementById("page").value= (parseInt(document.getElementById("page").value)+1) ;
                }
                function decrement(){
                    document.getElementById("page").value= (parseInt(document.getElementById("page").value)-1);
                    var count_from=parseInt(<%=count_from %>);
                    var count_to =parseInt(<%=count%>)
                    var result=count_from- count_to +count_from;
                    if(result>=1)
                        document.getElementById("listCount").value= (count_from- count_to +count_from);
                    console.log(document.getElementById("listCount").value);
                }

            </script>
            <form name="search criteria"
                  action="/simpleQuery" method="post">
                <input type="hidden" name="area" value=<%=area %>><br>
                <input type="hidden" name="price_from" value=<%=price_from %>><br>
                <input type="hidden" name="price_to" value=<%=price_to %>><br>
                <input type="hidden" name="beds" value=<%=beds %>><br>
                <input type="hidden" name="baths" value=<%=baths %>><br>
                <input type="hidden" id ="page" name="page" value=<%=page %>><br>
                <input type="hidden" id ="listCount" name="listCount" value=<%=listCount %>><br>
                <input type='submit' name='Previous Page' onclick="decrement()" value='previous' style='width:72px' />
                <input type='submit' name='Next Page' onclick="increment()" value='next' style='width:72px' />


            </form>


        </div>

    </div>
</div>
</div>
